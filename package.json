{
  "name": "translatr",
  "version": "3.0.3",
  "description": "A modern and intuitive i18n tool. Translatr simplifies i18n for developers and translators.",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "i18n:pull": "./src/python/translatr.py pull && cd public && ../src/python/translatr.py pull",
    "i18n:push": "./src/python/translatr.py push && cd public && ../src/python/translatr.py push",
    "release:patch": "ts-node ./src/typescript/release.ts patch",
    "release:minor": "ts-node ./src/typescript/release.ts minor",
    "release:major": "ts-node ./src/typescript/release.ts major"
  },
  "dependencies": {
    "backbone": "^1.3.3",
    "backbone-pageable": "^1.4.8",
    "backbone-undo": "^0.2.5",
    "codemirror": "^5.28.0",
    "getstream": "^3.5.0",
    "jquery": "^3.5.1",
    "materialize-css": "^0.100.2"
  },
  "devDependencies": {
    "@types/node": "14.0.14",
    "@types/semver": "^7.3.1",
    "semver": "7.3.2",
    "simple-git": "2.15.0",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.5"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/resamsel/translatr.git"
  },
  "keywords": [
    "i18n",
    "translation",
    "l10n",
    "translator",
    "messages",
    "java-properties",
    "playframework"
  ],
  "author": "Ren√© Panzar",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/resamsel/translatr/issues"
  },
  "homepage": "https://github.com/resamsel/translatr#readme"
}
