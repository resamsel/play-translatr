{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "",
  "projects": {
    "translatr": {
      "root": "apps/translatr",
      "sourceRoot": "apps/translatr/src",
      "projectType": "application",
      "prefix": "app",
      "schematics": {
        "@schematics/angular:component": {
          "styleext": "scss"
        }
      },
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/translatr",
            "index": "apps/translatr/src/index.html",
            "main": "apps/translatr/src/main.ts",
            "polyfills": "apps/translatr/src/polyfills.ts",
            "tsConfig": "apps/translatr/tsconfig.app.json",
            "assets": [
              "apps/translatr/src/favicon.png",
              "apps/translatr/src/assets"
            ],
            "styles": [
              "./node_modules/@angular/material/prebuilt-themes/purple-green.css",
              "apps/translatr/src/styles.scss",
              "apps/translatr/src/theme.scss",
              "apps/translatr/src/media.scss"
            ],
            "stylePreprocessorOptions": {
              "includePaths": [
                "apps/translatr/src/styles"
              ]
            },
            "scripts": [],
            "baseHref": "/ui/",
            "deployUrl": "/ui/"
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/translatr/src/environments/environment.ts",
                  "with": "apps/translatr/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "translatr:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "translatr:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "translatr:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "apps/translatr/src/test.ts",
            "polyfills": "apps/translatr/src/polyfills.ts",
            "tsConfig": "apps/translatr/tsconfig.spec.json",
            "karmaConfig": "apps/translatr/karma.conf.js",
            "styles": [
              "./node_modules/@angular/material/prebuilt-themes/purple-green.css",
              "apps/translatr/src/styles.scss",
              "apps/translatr/src/theme.scss",
              "apps/translatr/src/media.scss"
            ],
            "scripts": [],
            "assets": [
              "apps/translatr/src/favicon.png",
              "apps/translatr/src/assets"
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/translatr/tsconfig.app.json",
              "apps/translatr/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "translatr-e2e": {
      "root": "apps/translatr-e2e",
      "projectType": "application",
      "prefix": "",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "apps/translatr-e2e/protractor.conf.js",
            "devServerTarget": "translatr:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "translatr:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": "apps/translatr-e2e/tsconfig.e2e.json",
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    }
  },
  "defaultProject": "translatr",
  "cli": {
    "defaultCollection": "@nrwl/schematics"
  }
}
