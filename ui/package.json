{
  "name": "translatr",
  "version": "3.0.0-SNAPSHOT",
  "author": "Ren√© Panzar",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/resamsel/translatr/issues"
  },
  "homepage": "https://github.com/resamsel/translatr#readme",
  "scripts": {
    "ng": "ng",
    "prettier": "prettier --write \"**/*.{js,json,css,md,ts,html,component.html}\"",
    "prestart": "npm run buildinfo",
    "start": "ng serve --host 0.0.0.0 --port 4210",
    "start:admin": "ng serve translatr-admin --host 0.0.0.0 --port 4211",
    "start:docs": "ng serve docs --host 0.0.0.0 --port 4212",
    "generate": "dist/lets-generate",
    "test": "ng test translatr --watch=false --code-coverage=true",
    "test:ci": "ng test --watch=false",
    "test:coverage": "ng test --watch=true --code-coverage=true",
    "test:coverage:ci": "ng test --watch=false --code-coverage=true",
    "test:ui:coverage:ci": "ng test translatr --watch=false --code-coverage=true",
    "coveralls": "cat ./coverage/*/*/lcov.info | coveralls",
    "prebuild": "npm run buildinfo",
    "build": "ng build",
    "build:ui:dev": "ng build translatr --progress --output-path ../public/ui",
    "build:ui:prod": "ng build translatr --prod --output-path ../public/ui",
    "build:admin:dev": "ng build translatr-admin --progress --output-path ../public/admin",
    "build:admin:prod": "ng build translatr-admin --prod --output-path ../public/admin",
    "prebuild:prod": "npm run buildinfo",
    "build:prod": "npm run lint && npm run test:coverage:ci && npm run build:ui:prod && npm run build:admin:prod",
    "build:generate": "ng build cli-generate",
    "postbuild:generate": "tools/make-command.js dist/apps/cli-generate/main.js dist/lets-generate",
    "build:generate:prod": "ng build cli-generate --prod",
    "postbuild:generate:prod": "tools/make-command.js dist/apps/cli-generate/main.js dist/lets-generate",
    "buildinfo": "node tools/build-info.js",
    "lint": "./node_modules/.bin/nx workspace-lint && ng lint",
    "lint:fix": "./node_modules/.bin/nx lint --fix && ng lint --fix",
    "e2e": "ng e2e",
    "e2e:ui:watch": "ng e2e translatr-e2e --watch",
    "e2e:ui:prod": "ng e2e translatr-e2e --headless --prod",
    "postpull": "npm install",
    "precommit": "npm run build:prod",
    "affected:apps": "./node_modules/.bin/nx affected:apps",
    "affected:libs": "./node_modules/.bin/nx affected:libs",
    "affected:build": "./node_modules/.bin/nx affected:build",
    "affected:e2e": "./node_modules/.bin/nx affected:e2e",
    "affected:test": "./node_modules/.bin/nx affected:test",
    "affected:lint": "./node_modules/.bin/nx affected:lint",
    "affected:dep-graph": "./node_modules/.bin/nx affected:dep-graph",
    "affected": "./node_modules/.bin/nx affected",
    "format": "./node_modules/.bin/nx format:write",
    "format:write": "./node_modules/.bin/nx format:write",
    "format:check": "./node_modules/.bin/nx format:check",
    "update": "ng update @nrwl/workspace",
    "update:check": "ng update",
    "dep-graph": "./node_modules/.bin/nx dep-graph",
    "workspace-schematic": "./node_modules/.bin/nx workspace-schematic",
    "help": "./node_modules/.bin/nx help",
    "kill:server": "fuser -k 9000/tcp 4200/tcp 4211/tcp 4211/tcp; echo 'ports freed'",
    "nx": "nx",
    "prebundle-report": "ng build --prod --stats-json",
    "bundle-report": "webpack-bundle-analyzer dist/apps/translatr/stats-es2015.json",
    "i18n:extract": "transloco-keys-manager extract",
    "i18n:find": "transloco-keys-manager find"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^8.2.14",
    "@angular/cdk": "^8.2.3",
    "@angular/common": "^8.2.14",
    "@angular/compiler": "^8.2.14",
    "@angular/core": "^8.2.14",
    "@angular/forms": "^8.2.14",
    "@angular/material": "^8.2.3",
    "@angular/platform-browser": "^8.2.14",
    "@angular/platform-browser-dynamic": "^8.2.14",
    "@angular/router": "^8.2.14",
    "@ctrl/ngx-codemirror": "2.1.1",
    "@momothepug/tsmodule-alias": "^1.0.0",
    "@ngneat/transloco": "2.17.1",
    "@ngneat/transloco-messageformat": "1.2.1",
    "@ngrx/effects": "^8.6.0",
    "@ngrx/entity": "^8.6.0",
    "@ngrx/router-store": "^8.6.0",
    "@ngrx/store": "^8.6.0",
    "@nrwl/angular": "8.12.11",
    "@oclif/command": "1.6.1",
    "@oclif/config": "1.15.1",
    "@oclif/plugin-help": "^2.2.3",
    "codemirror": "5.53.2",
    "core-js": "^2.6.11",
    "d3": "5.16.0",
    "d3-time": "^1.1.0",
    "hammerjs": "^2.0.8",
    "jasmine-marbles": "^0.5.0",
    "messageformat": "2.3.0",
    "moment": "2.25.3",
    "ngx-gravatar": "^7.2.2",
    "ngx-moment": "^3.5.0",
    "random-name": "^0.1.2",
    "runes": "^0.4.3",
    "rxjs": "6.5.5",
    "tslib": "1.13.0",
    "underscore": "1.10.2",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.803.26",
    "@angular/cli": "8.3.26",
    "@angular/compiler-cli": "^8.2.14",
    "@angular/language-service": "^8.2.14",
    "@ngneat/transloco-keys-manager": "^1.0.0",
    "@ngneat/transloco-validator": "1.0.0",
    "@ngrx/store-devtools": "^8.6.0",
    "@nrwl/cypress": "8.12.11",
    "@nrwl/jest": "8.12.11",
    "@nrwl/linter": "8.12.11",
    "@nrwl/node": "8.12.11",
    "@nrwl/schematics": "8.12.11",
    "@nrwl/workspace": "8.12.11",
    "@oclif/dev-cli": "^1.22.2",
    "@schuchard/prettier": "^3.0.0",
    "@types/d3": "^5.7.2",
    "@types/d3-time": "^1.0.10",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "^2.0.8",
    "@types/jest": "^23.3.14",
    "@types/jquery": "3.3.6",
    "@types/node": "8.10.60",
    "@types/sanitize-html": "1.23.0",
    "@types/underscore": "1.10.0",
    "codelyzer": "5.2.2",
    "coveralls": "3.1.0",
    "cypress": "^3.8.3",
    "dateformat": "^3.0.3",
    "husky": "4.2.5",
    "jest": "^24.9.0",
    "jest-preset-angular": "7.1.1",
    "lint-staged": "9.5.0",
    "ngx-build-plus": "^9.0.2",
    "prettier": "^1.19.1",
    "rxjs-tslint-rules": "4.32.0",
    "ts-jest": "^24.3.0",
    "ts-node": "~7.0.0",
    "tsconfig-paths": "^3.9.0",
    "tslint": "~5.11.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "3.5.3",
    "webpack-bundle-analyzer": "3.7.0",
    "xmlhttprequest": "^1.8.0"
  },
  "files": [
    "/dist/lets-generate"
  ],
  "oclif": {
    "commands": "./apps/cli-generate/src/commands",
    "bin": "translatr",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "../public/i18n/*.json": [
      "transloco-validator"
    ]
  }
}
