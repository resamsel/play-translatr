{
  "name": "translatr",
  "version": "3.0.0-SNAPSHOT",
  "author": "Ren√© Panzar",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/resamsel/translatr/issues"
  },
  "homepage": "https://github.com/resamsel/translatr#readme",
  "scripts": {
    "ng": "ng",
    "prettier": "prettier --write \"**/*.{js,json,css,md,ts,html,component.html}\"",
    "prestart": "npm run buildinfo",
    "start": "ng serve --proxy-config apps/translatr/src/proxy.conf.js --host 0.0.0.0 --port 4210",
    "start:admin": "ng serve translatr-admin --proxy-config apps/translatr-admin/src/proxy.conf.js --host 0.0.0.0 --port 4211",
    "test": "ng test",
    "test:ci": "ng test --watch=false --browsers ChromeHeadless",
    "test:coverage": "ng test --watch=true --code-coverage=true",
    "test:coverage:ci": "ng test --watch=false --code-coverage=true --browsers ChromeHeadless",
    "prebuild": "npm run buildinfo",
    "build": "ng build",
    "build:ui:dev": "ng build translatr --progress --output-path ../public/ui",
    "build:ui:prod": "ng build translatr --progress --prod --output-path ../public/ui",
    "build:admin:dev": "ng build translatr-admin --progress --output-path ../public/admin",
    "build:admin:prod": "ng build translatr-admin --progress --prod --output-path ../public/admin",
    "prebuild:prod": "npm run buildinfo",
    "build:prod": "npm run build:ui:prod && npm run build:admin:prod",
    "buildinfo": "node tools/build-info.js",
    "lint": "./node_modules/.bin/nx lint && ng lint",
    "e2e": "ng e2e",
    "affected:apps": "./node_modules/.bin/nx affected:apps",
    "affected:libs": "./node_modules/.bin/nx affected:libs",
    "affected:build": "./node_modules/.bin/nx affected:build",
    "affected:e2e": "./node_modules/.bin/nx affected:e2e",
    "affected:test": "./node_modules/.bin/nx affected:test",
    "affected:lint": "./node_modules/.bin/nx affected:lint",
    "affected:dep-graph": "./node_modules/.bin/nx affected:dep-graph",
    "affected": "./node_modules/.bin/nx affected",
    "format": "./node_modules/.bin/nx format:write",
    "format:write": "./node_modules/.bin/nx format:write",
    "format:check": "./node_modules/.bin/nx format:check",
    "update": "ng update @nrwl/schematics",
    "update:check": "ng update",
    "dep-graph": "./node_modules/.bin/nx dep-graph",
    "workspace-schematic": "./node_modules/.bin/nx workspace-schematic",
    "help": "./node_modules/.bin/nx help",
    "tools:generatr": "ts-node --project tools/generatr/tsconfig.json -r tsconfig-paths/register tools/generatr/generatr.ts"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^7.1.4",
    "@angular/cdk": "^7.3.3",
    "@angular/common": "~7.1.0",
    "@angular/compiler": "~7.1.0",
    "@angular/core": "~7.1.0",
    "@angular/forms": "~7.1.0",
    "@angular/material": "^7.3.3",
    "@angular/platform-browser": "~7.1.0",
    "@angular/platform-browser-dynamic": "~7.1.0",
    "@angular/router": "~7.1.0",
    "@ctrl/ngx-codemirror": "^1.3.10",
    "@ngrx/effects": "7.2.0",
    "@ngrx/entity": "7.2.0",
    "@ngrx/router-store": "7.2.0",
    "@ngrx/store": "7.2.0",
    "@nrwl/nx": "^7.6.2",
    "@types/sanitize-html": "^1.18.2",
    "@types/underscore": "^1.8.14",
    "codemirror": "^5.44.0",
    "core-js": "^2.5.4",
    "frappe-charts": "^1.1.0",
    "hammerjs": "^2.0.8",
    "moment": "^2.24.0",
    "ngx-gravatar": "^7.2.2",
    "ngx-moment": "^3.3.0",
    "runes": "^0.4.3",
    "rxjs": "~6.3.3",
    "tslib": "^1.9.0",
    "underscore": "^1.9.1",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.11.0",
    "@angular/cli": "~7.1.4",
    "@angular/compiler-cli": "~7.1.0",
    "@angular/http": "^7.2.13",
    "@angular/language-service": "~7.1.0",
    "@ngrx/store-devtools": "7.2.0",
    "@nrwl/builders": "7.6.2",
    "@nrwl/schematics": "^7.6.2",
    "@schuchard/prettier": "^3.0.0",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/jest": "23.0.0",
    "@types/jquery": "3.3.6",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.5.0",
    "dateformat": "^3.0.3",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "jest": "23.0.0",
    "jest-preset-angular": "6.0.1",
    "karma": "~3.1.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "lint-staged": "8.1.5",
    "ngrx-store-freeze": "0.2.4",
    "prettier": "^1.16.4",
    "protractor": "~5.4.0",
    "random-name": "^0.1.2",
    "ts-node": "~7.0.0",
    "tsconfig-paths": "^3.8.0",
    "tslint": "~5.11.0",
    "tslint-config-prettier": "1.18.0",
    "typescript": "~3.1.6",
    "xmlhttprequest": "^1.8.0"
  }
}
